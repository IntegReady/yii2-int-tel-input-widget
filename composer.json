{
    "name": "integready/yii2-intl-tel-input",
    "version": "0.1.2",
    "description": "Yii2 International telephone numbers - Asset Bundle, Behavior, Validator, Widget",
    "keywords": [
        "yii2",
        "intl-tel-input",
        "international telephone numbers",
        "libphonenumber"
    ],
    "minimum-stability": "stable",
    "license": "BSD-3-Clause",
    "type": "yii2-extension",
    "homepage": "https://github.com/IntegReady/yii2-intl-tel-input",
    "support": {
        "issues": "https://github.com/IntegReady/yii2-intl-tel-input/issues?state=open",
        "source": "https://github.com/IntegReady/yii2-intl-tel-input"
    },
    "autoload": {
        "psr-4": {
            "integready\\extensions\\phoneInput\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "bower-asset/intl-tel-input": "^16",
        "giggsey/libphonenumber-for-php": "^8",
        "yiisoft/yii2": ">=2.0.1"
    }
}
